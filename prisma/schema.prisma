// #02

model car_owners {
  id             Int     @id @default(autoincrement())
  bio            String?
  car_color      String?
  car_model      String?
  car_model_year String?
  country        String?
  email          String?
  first_name     String?
  gender         String?
}

model users {
  id         Int     @id @default(autoincrement())
  first_name String?
  last_name  String?
  email      String?
  address    String?
}
