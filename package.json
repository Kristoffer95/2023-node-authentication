{
  "name": "prisma-node-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "predev": "npm run build",
    "dev": "ts-node-dev --poll src/index.ts",
    "migrate": "prisma migrate dev --name",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev --skip-seed",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma reset",
    "predocker:up": "rm -rf docker-data",
    "docker:up": "docker compose -f docker-compose.dev.yml up --build",
    "docker:up:detached": "docker compose -f docker-compose.dev.yml up --build -d",
    "predocker:down": "rm -rf docker-data",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:start": "docker compose -f docker-compose.dev.yml start",
    "docker:stop": "docker compose -f docker-compose.dev.yml stop",
    "docker:restart": "docker compose -f docker-compose.dev.yml restart",
    "docker:logs": "docker-compose logs -f"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.7",
    "@types/ioredis": "^5.0.0",
    "@types/morgan": "^1.9.4",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.0",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@prisma/client": "^5.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.10",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
